        "@playwright-orchestrator/pg": {
            "optional": true
        },
        "@playwright-orchestrator/mysql": {
            "optional": true
        },
        "@playwright-orchestrator/mongo": {
            "optional": true
        }
    },
    "scripts": {
        "prepare": "cp ../../README.md ./ && cp ../../LICENSE.md ./"
