{
    "name": "@playwright-orchestrator/core",
    "version": "1.1.3",
    "description": "Core lib and cli for Playwright test orchestration",
    "keywords": [
        "playwright",
        "testing",
        "automation",
        "orchestration",
        "e2e"
    ],
    "author": "Rostyslav Kudrevatykh",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rostmanrk/playwright-orchestrator.git"
    },
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "playwright-orchestrator": "./dist/cli.js"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@commander-js/extra-typings": "^13.0.0",
        "ansi-escapes": "^7.0.0",
        "chalk": "^5.4.1",
        "commander": "^13.0.0",
        "boxen": "^8.0.1",
        "typescript": "^5.0.0",
        "uuid": "^11.0.3"
    },
    "devDependencies": {},
    "type": "module",
