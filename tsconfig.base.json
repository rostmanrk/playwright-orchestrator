{
    "compilerOptions": {
        "target": "es2022",
        "module": "Node16",
        "moduleResolution": "node16",
        "lib": ["ES2023"],
        "strict": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "allowJs": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "declaration": true,
        "sourceMap": true,
        "composite": true
    }
}
